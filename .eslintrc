{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
        "func-names": ["error", "never"],
        "max-len": ["error", { "ignoreTrailingComments": true, "ignoreComments": true, "ignoreStrings": true, "code": 200 }],
        "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["ast"] }],
        "no-shadow":"off",
        "consistent-return": ["error", { "treatUndefinedAsUnspecified": true }],
        "no-nested-ternary":"off",
        "no-useless-escape":"off",
        "no-prototype-builtins":"off"
    }
}
